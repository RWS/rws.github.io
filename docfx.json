{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }     
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.json",
          "**/*.html"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "RWSTemplate"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_enableSearch": true
    },
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}